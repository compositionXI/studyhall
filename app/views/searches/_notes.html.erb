<% @search.notes(params[:page] || 1).results.each do |search_result| %>
  <%= render :partial => "note", :locals => {:note => search_result} %>
<% end %>
<%= link_to_next_page @search.notes(params[:page] || 1).results, "See more results", class: 'more_results', remote: true, params: { category: "notes"} %>
