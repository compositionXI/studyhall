linkElement = $("#<%=j @modal_link_id %>");
linkElement.popover = null;
$("#<%= @modal_link_id %>").popover({
  trigger: "manual",
  placement: "below-right",
  offset: 10,
  horizontalOffset: 25,
  html: true,
  title: function(){return "New Studyhall";},
  content: function(){return "<%=j render :partial => 'form' %>";}
});
$("#<%= @modal_link_id %>").popover("show");
$("#new_study_session input[type='text']").first().focus();