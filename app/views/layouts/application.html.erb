<!DOCTYPE html>
<html>
<head>
  <title>Studyhall</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div>
    <%= link_to "Login", root_path if @current_user.nil? %>
    <%= link_to "Logout", logout_path if @current_user %>
    <%= link_to "StaticPages", static_pages_path if @current_user && @current_user.admin? %>
    <%= link_to "Contact", new_contact_path %>
    <% @static_pages.each do |static_page| %>
      <%= link_to static_page.title, page_path(static_page.slug) %>
    <% end %>
  </div>
  <%= yield %>

</body>
</html>
