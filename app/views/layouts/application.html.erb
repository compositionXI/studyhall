<!DOCTYPE html>
<html>
<head>
  <title>Studyhall</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div>
    <%= link_to "Login", root_path if @current_user.nil? %>
    <%= link_to "Logout", logout_path if @current_user %>
    <%= link_to "About", about_public_path(about_permalink) %>
    <%= link_to "Contact", new_contact_path %>
    <%= link_to "FAQs", faqs_path %>
  </div>
  <%= yield %>

</body>
</html>
